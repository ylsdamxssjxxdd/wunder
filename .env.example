# Copy to .env and adjust values for your environment.
WUNDER_HOST=0.0.0.0
WUNDER_PORT=18000
WUNDER_SERVER_MODE=api
WUNDER_LOG_LEVEL=info
WUNDER_API_KEY=ylsdamxssjxxdd
WUNDER_GATEWAY_TOKEN=ylsdamxssjxxdd
# Use postgres service name inside docker, or 127.0.0.1 for local.
WUNDER_POSTGRES_DSN=postgresql://wunder:wunder@wunder-postgres:5432/wunder
WUNDER_POSTGRES_POOL_SIZE=64
WUNDER_POSTGRES_PORT=15432
MCP_PORT=9010
# Weaviate vector store.
WUNDER_WEAVIATE_URL=http://wunder-weaviate:8080
WUNDER_WEAVIATE_API_KEY=
WEAVIATE_PORT=18003
# Sandbox endpoint (docker default).
WUNDER_SANDBOX_ENDPOINT=http://wunder-sandbox:9001
# Sandbox concurrency tuning.
WUNDER_SANDBOX_WORKERS=8
WUNDER_SANDBOX_THREADPOOL_WORKERS=32
# Workspace root inside container (docker default).
WUNDER_WORKSPACE_ROOT=/workspaces
# Frontend (user) dev server.
FRONTEND_HOST=0.0.0.0
FRONTEND_PORT=18001
VITE_API_BASE=/wunder
VITE_DEV_PROXY_TARGET=http://wunder-server:18000
CHOKIDAR_USEPOLLING=1
NGINX_PORT=18002
